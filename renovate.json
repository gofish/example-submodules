{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended",
    "group:all",
    ":rebaseStalePrs"
  ],
  "packageRules": [
    {
      "matchUpdateTypes": [
        "major",
        "minor",
        "patch",
        "pin",
        "digest"
      ],
      "automerge": false
    }
  ],
  "timezone": "America/Los_Angeles",
  "schedule": [
    "every weekday"
  ]
}
